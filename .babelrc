{
    "presets": [
        [
            "minify", {
                "builtIns": false,
                "evaluate":false,
                "mangle":false
            }
        ],
        [
            "@babel/preset-env",{
            // los targets son las plataformas
            // donde se ejecutara  el codigo final
                "targets": {"node" : "current"}
            }
        ]
    ],
    "plugins":[
        [
            "module-resolver", {
                "root": ["./"],
                "alias": {
                    "@server":"./server"
                }
            }
        ]
    ]
}